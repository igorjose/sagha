%p.alert.alert-success= notice

%h1 Visualizar software utilizado pelo órgão

%table.table.table-hover
  %tr
    %td
      %b Órgão:
    %td
      = @orgao_software.orgao_id
  %tr
    %td
      %b Software:
    %td
      = @orgao_software.software_id
  %tr
    %td
      %b Quantidade:
    %td
      = @orgao_software.quantidade

%br

.btn.btn-small.btn-inverse
  = link_to 'Editar', edit_orgao_software_path(@orgao_software)
.btn.btn-small.btn-inverse
  = link_to 'Voltar', orgao_softwares_path